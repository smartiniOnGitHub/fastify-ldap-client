{
  "name": "fastify-ldap-client",
  "version": "0.1.0",
  "description": "Fastify Plugin to connect and authenticate to LDAP servers",
  "main": "src/plugin",
  "scripts": {
    "example": "node example/example",
    "example-enhanced": "node example/example-enhanced",
    "lint": "standard \"./*.js\" \"./src/**/*.js\" \"./test/**/*.test.js\" \"./example/**/*.js\"",
    "test:unit": "tap -J test/*.test.js test/*/*.test.js",
    "test:unit:debug": "tap -T --strict --node-arg=--inspect-brk test/*.test.js test/*/*.test.js",
    "test": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "fastify-plugin": "^1.6.1",
    "ldapjs-client": "^0.1.1"
  },
  "devDependencies": {
    "fastify": "^2.15.3",
    "standard": "^14.3.4",
    "tap": "^14.10.8"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=8.17.0"
  },
  "homepage": "https://github.com/smartiniOnGitHub/fastify-ldap-client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartiniOnGitHub/fastify-ldap-client.git"
  },
  "bugs": {
    "url": "https://github.com/smartiniOnGitHub/fastify-ldap-client/issues"
  },
  "keywords": [
    "fastify",
    "plugin",
    "ldap"
  ],
  "author": "Sandro Martini <sandro.martini@gmail.com>",
  "license": "MIT"
}
